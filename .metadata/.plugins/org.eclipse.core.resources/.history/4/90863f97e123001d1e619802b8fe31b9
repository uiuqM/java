package entities;

public class Pessoa {
	private String nome;
	private Double rendaAnual;
	
	public Pessoa() {
	}
	public Pessoa(String nome, Double rendaAnual) {
		this.nome = nome;
		this.rendaAnual = rendaAnual;
	}
	
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
}
